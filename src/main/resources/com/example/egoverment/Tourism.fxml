               <font>
                  <Font name="Javanese Text" size="24.0" />
               </font>
            </Label>
            <ImageView fx:id="tut1111" fitHeight="162.0" fitWidth="238.0" layoutX="29.0" layoutY="969.0" pickOnBounds="true" preserveRatio="true">
