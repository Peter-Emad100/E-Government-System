<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<ScrollPane xmlns:fx="http://javafx.com/fxml/1" fx:id="scrollPane" prefHeight="400.0" prefWidth="600.0">
    <content>
        <VBox spacing="10" alignment="CENTER">
            <ImageView fitWidth="500" fitHeight="500" preserveRatio="true">
                <image>
                    <Image url="@assets/prim_Arabic.jpg"/>
                </image>
            </ImageView>

            <ImageView fitWidth="500" fitHeight="500" preserveRatio="true">
                <image>
                    <Image url="@assets/prim_english.jpg"/>
                </image>
            </ImageView>

            <ImageView fitWidth="500" fitHeight="500" preserveRatio="true">
                <image>
                    <Image url="@assets/prim_maths.jpg"/>
                </image>
            </ImageView>

            <!-- Add more images as needed -->
        </VBox>
    </content>
</ScrollPane>